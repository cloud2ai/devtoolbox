# ============================================================================
# LLM Module Configuration
# ============================================================================

# OpenAI API Configuration
# Used for accessing OpenAI API services, including GPT-3.5/4 models
OPENAI_API_KEY=your-openai-api-key

# Azure OpenAI Configuration
# Used for accessing Azure OpenAI services, including GPT-3.5/4 models
AZURE_OPENAI_API_KEY=your-azure-openai-api-key
AZURE_OPENAI_ENDPOINT=your-azure-openai-endpoint
AZURE_OPENAI_API_VERSION=2024-02-15-preview

# DeepSeek API Configuration
# Used for accessing DeepSeek API services
DEEPSEEK_API_KEY=your-deepseek-api-key

# ============================================================================
# OCR Module Configuration
# ============================================================================

# Azure Document Intelligence Configuration
# Used for accessing Azure Document Intelligence service, providing OCR functionality
AZURE_DOCUMENT_INTELLIGENCE_ENDPOINT=your-azure-document-intelligence-endpoint
AZURE_DOCUMENT_INTELLIGENCE_KEY=your-azure-document-intelligence-key

# ============================================================================
# Speech Processing Module Configuration
# ============================================================================

# Azure Speech Services Configuration
# Used for accessing Azure Speech services, providing speech-to-text and text-to-speech functionality
AZURE_SPEECH_KEY=your-azure-speech-key
AZURE_SPEECH_REGION=your-azure-speech-region

# Azure Storage configuration is required because audio files longer than
# one minute must be uploaded to S3-compatible storage before Azure can
# process them for speech recognition.
AZURE_STORAGE_ACCOUNT=your-azure-storage-account
AZURE_STORAGE_KEY=your-azure-storage-key

# VolcEngine Speech Services Configuration
# Used for accessing VolcEngine Speech services
VOLC_ACCESS_KEY=your-volc-access-key
VOLC_SECRET_KEY=your-volc-secret-key
VOLC_APP_ID=your-volc-app-id

# ============================================================================
# Storage Module Configuration
# ============================================================================

# Object Storage Configuration
# Used for accessing object storage services, such as S3-compatible storage
STORAGE_ENDPOINT=your-storage-endpoint
STORAGE_ACCESS_KEY=your-storage-access-key
STORAGE_SECRET_KEY=your-storage-secret-key
STORAGE_BUCKET=your-storage-bucket

# ============================================================================
# Jira Module Configuration
# ============================================================================

# Jira Service Configuration
# Used for accessing Jira services, providing project management functionality
JIRA_URL=your-jira-server-url
JIRA_USERNAME=your-jira-username
JIRA_API_TOKEN=your-jira-api-token

# ============================================================================
# Development Environment Configuration
# ============================================================================

# Development Environment Configuration
# Used for controlling development environment behavior
DEBUG=false
LOG_LEVEL=INFO
CACHE_DIR=.cache